{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "call-api-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/callApi/CallApiWebPart.js",
          "manifest": "./src/webparts/callApi/CallApiWebPart.manifest.json"
        }
      ]
    },
    "teams-tab-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/teamsTab/TeamsTabWebPart.js",
          "manifest": "./src/webparts/teamsTab/TeamsTabWebPart.manifest.json"
        }
      ]
    },
    "feed-back-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/feedBack/FeedBackWebPart.js",
          "manifest": "./src/webparts/feedBack/FeedBackWebPart.manifest.json"
        }
      ]
    },
    "permission-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/permission/PermissionWebPart.js",
          "manifest": "./src/webparts/permission/PermissionWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CallApiWebPartStrings": "lib/webparts/callApi/loc/{locale}.js",
    "TeamsTabWebPartStrings": "lib/webparts/teamsTab/loc/{locale}.js",
    "FeedBackWebPartStrings": "lib/webparts/feedBack/loc/{locale}.js",
    "PermissionWebPartStrings": "lib/webparts/permission/loc/{locale}.js"
  }
}
